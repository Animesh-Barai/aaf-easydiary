    android:padding="3dp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <me.blog.korn123.easydiary.views.CalendarItem
        android:id="@+id/label"
        android:textSize="@dimen/calendar_item_text_default"
